        <Button Content="Yellow" Click="Button_Clicked" HorizontalAlignment="Left" Margin="51,138,0,0" VerticalAlignment="Top" Height="48" Width="48"/>
        <Button Content="Green" Click="Button_Clicked" HorizontalAlignment="Left" Margin="51,225,0,0" VerticalAlignment="Top" Height="48" Width="48"/>
        <Button Content="Go Back" Click="Button_Clicked" HorizontalAlignment="Left" Height="127" Margin="490,78,0,0" VerticalAlignment="Top" Width="128" Background="#FFF382E5"/>

    </Grid>
</Window>
